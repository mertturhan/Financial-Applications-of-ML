    "_opts    = Options()\n",
    "_opts.add_argument(\"--headless\")\n",
    "_service = Service(executable_path=_path)\n",
    "\n",
    "driver = webdriver.Chrome(service=_service, options=_opts)\n",
    "driver.implicitly_wait(5)"
